{{#extend "layout"}}
    {{#replace "main"}}
        <section id="landing">
            <div class="inner">
                <h1>Sam's Cool Artwork Gallery</h1>
                <h2>A Journey Through the Works of a Young Artist</h2>
                <h3>metro trains</h3>
                <div class="fotorama" data-nav="thumbs" data-allowfullscreen="true" data-width="100%" data-ratio="800/400" data-loop="true">
                    <img src="../assets/img/sam/self-portrait.jpg" alt="sam self portrait">
                    <img src="../assets/img/sam/sam-shapes.jpg" alt="sam shapes">
                    <img src="../assets/img/sam/minion-train.jpg" alt="minion train">
                    <img src="../assets/img/sam/tropical-island.jpg" alt="tropical island">
                    <img src="../assets/img/sam/poking-tongue.jpg" alt="poking tongue">
                    <img src="../assets/img/sam/fruit-basket-rs01.jpg" alt="Fruit Basket">
                    <img src="../assets/img/sam/villiage-rs01.jpg">
                    <img src="../assets/img/sam/four-houses-to-make-a-village.jpg" alt="Four Houses to Make a Village">
                </div>
            </div>
        </section>

    {{/replace}}
{{/extend}}
